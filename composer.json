
{
    "name": "peyman-manteali/telegram-notifier",
    "description": "A simple Laravel package to send messages to Telegram using the Telegram Bot API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "peyman-manteali",
            "email": "peyman.manteali@gmail.com"
        }
    ],
    "require": {
        "php": ">=8",
        "guzzlehttp/guzzle": "7",
        "illuminate/support": ">=10"
    },
    "autoload": {
        "psr-4": {
            "TelegramNotifier\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
